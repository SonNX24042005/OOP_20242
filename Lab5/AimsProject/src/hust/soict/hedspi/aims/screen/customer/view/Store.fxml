<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets left="20.0" />
   </padding>
   <children>
      <HBox alignment="CENTER" prefHeight="100.0">
         <children>
            <Label text="AIMS">
               <font>
                  <Font size="50.0" />
               </font>
               <textFill>
                  <javafx.scene.paint.Color red="0.0" green="0.298" blue="1.0" /> </textFill>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="btnViewCart" mnemonicParsing="false" onAction="#btnViewCartPressed" prefHeight="50.0" prefWidth="100.0" text="View Cart">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ScrollPane prefHeight="760.0" prefWidth="1024.0">
         <content>
            <GridPane fx:id="gridPane" prefHeight="0.0" prefWidth="0.0">
              </GridPane>
         </content>
      </ScrollPane>
   </children>
</VBox>